<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <style>
        body {margin: 0; background: #6af68f;}
        .users {
            display: flex;
            flex-direction: column;
            margin-top: 10px;
            padding: 5px 25px;
            background: #26c07b;
        }
        .users .item {
            font-size: 20px;
        }
    </style>
</head>
<body>
        {{#each users}}
    <div class="users">
            <div class="item">First Name: {{this.firstName}}</div>
            <div class="item">Last Name: {{this.lastName}}</div>
            <div class="item">Email: {{this.email}}</div>
            <div class="item">Password: {{this.password}}</div>
            <div class="item">Age: {{this.age}}</div>
            <div class="item">City: {{this.city}}</div>
    </div>
        {{/each}}
</body>
</html>